{
  "name": "svelte-meta-tags",
  "version": "4.5.0",
  "description": "Svelte Meta Tags provides components designed to help you manage SEO for Svelte projects",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oekazuma/svelte-meta-tags.git",
    "directory": "packages/svelte-meta-tags"
  },
  "homepage": "https://oekazuma.github.io/svelte-meta-tags/",
  "dependencies": {
    "schema-dts": "^1.1.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.47.2",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "publint": "^0.3.15",
    "svelte": "^5.41.1",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "exports": {
    "./JsonLd.svelte": {
      "types": "./dist/JsonLd.svelte.d.ts",
      "svelte": "./dist/JsonLd.svelte"
    },
    "./MetaTags.svelte": {
      "types": "./dist/MetaTags.svelte.d.ts",
      "svelte": "./dist/MetaTags.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:bench": "vitest bench"
  }
}